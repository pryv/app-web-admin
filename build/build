#!/usr/bin/env bash

set -e

version=1.0.4
host='docker.io'

echo "---------------------------- building mfa --------------------------------"
echo $host/pryvio/app-web-auth3:$version
docker build --build-arg COMPONENT_NAME=app-web-auth3 -f build/Dockerfile -t $host/pryvio/app-web-auth3:$version .
docker push $host/pryvio/app-web-auth3:$version
